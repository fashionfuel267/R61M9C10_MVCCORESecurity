@model IEnumerable< Microsoft.AspNetCore.Identity.IdentityRole>
<a asp-action="Create" asp-controller="ManageRole" >Create</a>
<table class="table table-bordered">
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>...</th>
	</tr>
	@foreach(var item in Model){
		<tr>
			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>
				<a asp-action="Edit" asp-controller="ManageRole" asp-route-id="@item.Id">Edit</a>
				<a asp-action="Delete" asp-controller="ManageRole" asp-route-id="@item.Id">Delete</a>
			</td>
		</tr>
	}
</table>