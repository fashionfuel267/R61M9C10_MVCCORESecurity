
@{
    ViewData["Title"] = "Create";
}
@if (ViewBag.errorMsg!=null){
    @ViewBag.errorMsg
}
<h1>Create Role</h1>
<form method="post" asp-action="Create" asp-controller="ManageRole"  >

    <div class="form-group">
        
        <label for="rolename"> Name</label>

        <input type="text" class="form-control" name="rolename" id="rolename" aria-describedby="emailHelp" placeholder="Enter Role name">

    </div>
     
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

